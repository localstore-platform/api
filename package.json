{
  "name": "@localstore/api",
  "version": "1.0.0",
  "description": "LocalStore Platform API - Vietnamese-first multi-tenant SaaS backend",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:api": "newman run postman/localstore-api.postman_collection.json -e postman/localstore-dev.postman_environment.json",
    "test:api:report": "newman run postman/localstore-api.postman_collection.json -e postman/localstore-dev.postman_environment.json -r htmlextra --reporter-htmlextra-export reports/api-test-report.html",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:prod": "node ./node_modules/typeorm/cli.js",
    "migration:generate": "pnpm typeorm migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "pnpm typeorm migration:run -d src/config/typeorm.config.ts",
    "migration:run:prod": "pnpm typeorm:prod migration:run -d dist/migrations/src/config/typeorm.config.js",
    "migration:revert": "pnpm typeorm migration:revert -d src/config/typeorm.config.ts",
    "migration:revert:prod": "pnpm typeorm:prod migration:revert -d dist/migrations/src/config/typeorm.config.js",
    "migration:build": "tsc -p tsconfig.migrations.json",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/run-seed.ts",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:up:seed": "docker compose -f docker-compose.dev.yml --profile seed up -d --build --force-recreate",
    "docker:down": "docker compose -f docker-compose.dev.yml --profile seed --profile tools down -v --remove-orphans",
    "docker:build": "docker compose -f docker-compose.dev.yml build",
    "docker:logs": "docker logs localstore-api",
    "docker:logs:save": "mkdir -p logs && docker logs localstore-api > logs/api.log 2>&1",
    "docker:logs:follow": "docker logs -f localstore-api",
    "docker:tools": "docker compose -f docker-compose.dev.yml --profile tools up -d",
    "docker:reset": "pnpm docker:down && pnpm docker:up:seed"
  },
  "keywords": [
    "nestjs",
    "typescript",
    "api",
    "localstore",
    "vietnam",
    "multi-tenant"
  ],
  "author": "LocalStore Platform",
  "license": "AGPL-3.0",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@localstore/contracts": "^0.2.0",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "newman": "^6.2.1",
    "newman-reporter-htmlextra": "^1.23.1",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
